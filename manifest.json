{
	"name": "Who CORS?",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Allows to you request any site with ajax from any source. Adds to response 'Access-Control-Allow-Origin: *' header",
	"background": {
		"scripts": [
			"global.js",
			"config.js",
			"icon.js",
			"listeners.js",
			"activate.js"
		]
	},
	"browser_action": {
		"default_icon": "off.png",
		"default_title": "Who CORS?",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
    "https://fonts.googleapis.com/",
		"https://maxcdn.bootstrapcdn.com/"
	],
	"web_accessible_resources": [
		"on.png",
		"off.png"
	]
}
